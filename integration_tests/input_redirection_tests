tr a-z A-Z <  ../CMakeLists.txt && echo passed
tr a-z A-Z < ../CMakeLists2.txt || echo passed
tr a-z A-Z < ../names.txt && echo passed
tr a-z A-Z < ../names3.txt || echo passed
wc < ../CMakeLists.txt && echo passed
wc < ../CMakeLists2.txt || echo passed
wc < ../names.txt && echo passed
wc < ../names3.txt || echo passed
exit
